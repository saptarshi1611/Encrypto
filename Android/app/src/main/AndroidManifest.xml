<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="securedqr.smit.edu.in.app">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/cryptography"
        android:label="@string/app_name" >
        <activity
            android:name="securedqr.smit.edu.in.app.util.QRCode"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="securedqr.smit.edu.in.app.util.Verify"
            android:label="@string/title_activity_ver_sig"
            android:screenOrientation="portrait">
            <!--<intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>-->
        </activity>
        <activity
            android:name="securedqr.smit.edu.in.app.util.GenQR"
            android:label="@string/title_activity_gen_qr"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="securedqr.smit.edu.in.app.util.DecodeQR"
            android:label="@string/title_activity_decode_qr"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>